---
import BaseLayout from "../layouts/BaseLayout.astro"

import PriceTop from "../components/pages/price/PriceTop.astro"
import PriceRates from "../components/pages/price/PriceRates.astro"
import PriceTeachers from "../components/pages/price/PriceTeachers.astro"
import PriceLead from "../components/pages/price/PriceLead.astro"

const base = import.meta.env.BASE_URL
---

<BaseLayout title="Вартість навчання">
  <PriceTop base={base} />
  <PriceRates base={base} />
  <PriceTeachers base={base} />
  <PriceLead base={base} />
</BaseLayout>
